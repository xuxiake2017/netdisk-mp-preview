.custom-progress {
  width: 100%;
}
.custom-progress__content {
  width: 100%;
  height: 16rpx;
  border-radius: 8rpx;
  background: #F9F9F9;
  position: relative;
}
.custom-progress__bar {
  position: absolute;
  left: 0;
  top: 0;
  border-radius: 8rpx;
  height: 100%;
  width: 0;
  background: #FD9600;
  transition: all ease-in-out .4s;
}
.custom-progress__desc {
  color: #9E9E9E;
  line-height: 2.5;
  font-size: 26rpx;
}